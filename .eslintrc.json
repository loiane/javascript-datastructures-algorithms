{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2016": true,
    "node": true
  },
  "globals": {
    "expect": true,
    "it": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "document": false,
    "navigator": false,
    "window": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "extends": "airbnb-base",
  "rules": {
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-restricted-globals": 0,
    "no-multi-assign": 0,
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "padded-blocks": 0,
    "no-sparse-arrays": 0,
    "array-bracket-spacing": 0,
    "import/no-named-as-default": 0,
    "implicit-arrow-linebreak": 0
  }
}
